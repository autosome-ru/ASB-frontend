{
  "name": "ad-astra",
  "version": "0.0.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ananas": "node --max_old_space_size=1000 ./node_modules/@angular/cli/bin/ng build ananas --configuration production --output-hashing=all",
    "dev:ananas": "ng serve ananas",
    "dev:ssr": "ng run ad-astra:serve-ssr",
    "serve:ssr": "node dist/adastra/server/main.js",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/adastra/browser/stats.json",
    "build:ssr": "node --max_old_space_size=1000 ./node_modules/@angular/cli/bin/ng build --configuration production --output-hashing=all && node --max_old_space_size=1000 ./node_modules/@angular/cli/bin/ng run ad-astra:server:production",
    "prerender": "ng run ad-astra:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.6",
    "@angular/cdk": "^13.2.6",
    "@angular/common": "~13.2.6",
    "@angular/compiler": "~13.2.6",
    "@angular/core": "~13.2.6",
    "@angular/forms": "~13.2.6",
    "@angular/localize": "^13.2.6",
    "@angular/material": "^12.2.6",
    "@angular/platform-browser": "~13.2.6",
    "@angular/platform-browser-dynamic": "~13.2.6",
    "@angular/platform-server": "~13.2.6",
    "@angular/router": "~13.2.6",
    "@angular/service-worker": "~13.2.6",
    "@ng-bootstrap/ng-bootstrap": "^12.0.0",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@nguniversal/express-engine": "^13.0.2",
    "angular-bootstrap-md": "^12.2.0",
    "core-js": "^3.9.1",
    "express": "^4.17.1",
    "file-saver": "^2.0.5",
    "ngx-filesaver": "^12.0.0",
    "ngx-joyride": "^2.4.0",
    "ngx-toastr": "^14.1.3",
    "rxjs": "~6.6.6",
    "tslib": "^2.3.1",
    "zone": "^0.3.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.2.6",
    "@angular/cli": "~13.2.6",
    "@angular/compiler-cli": "~13.2.6",
    "@angular/language-service": "~13.2.6",
    "@ngrx/store-devtools": "^13.0.2",
    "@nguniversal/builders": "^13.0.2",
    "@types/chart.js": "^2.9.35",
    "@types/file-saver": "^2.0.5",
    "@types/express": "^4.17.13",
    "@types/jasmine": "~3.9.1",
    "@types/jasminewd2": "~2.0.8",
    "@types/lodash": "^4.14.173",
    "@types/node": "14.11.2",
    "body-parser": "^1.19.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~9.0.0",
    "tsc-watch": "^4.2.9",
    "tslint": "~6.1.3",
    "typescript": "4.5.5",
    "webpack": "^5.53.0",
    "webpack-bundle-analyzer": "^3.9.0"
  }
